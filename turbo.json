{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ],
      "inputs": []
    },
    "@micro-stacks/docs#build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**"
      ],
      "inputs": [
        "theme.config.js",
        "next.config.js",
        "middleware.ts",
        "pages/**",
        "common/**",
        "components/**",
        "package.json"
      ]
    },
    "test": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "ci:publish": {
      "cache": false
    },
    "ci:publish:packages": {
      "cache": false
    },
    "dev": {
      "cache": false
    }
  }
}

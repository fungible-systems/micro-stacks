  "name": "micro-stacks",
  "private": false,
  "version": "0.5.4",
  "description": "Tiny libraries for building Stacks apps.",
  "main": "index.js",
  "module": "index.mjs",
  "import": "index.mjs",
  "react-native": "index.js",
  "types": "index.d.ts",

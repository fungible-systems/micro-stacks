    "@typescript-eslint/eslint-plugin": "5.1.0",
    "@typescript-eslint/parser": "5.1.0",
    "bip32": "2.0.6",
    "common-tags": "1.8.0",
    "concurrently": "6.3.0",
    "cross-fetch": "3.1.4",
    "dependency-cruiser": "10.5.0",
    "downlevel-dts": "0.7.0",
    "esbuild": "0.13.8",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.25.2",

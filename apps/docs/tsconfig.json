{
  "compilerOptions": {
	"target": "es5",
	"lib": [
	  "dom",
	  "dom.iterable",
	  "esnext"
	],
	"allowJs": true,
	"skipLibCheck": true,
	"strict": false,
	"forceConsistentCasingInFileNames": true,
	"noEmit": true,
	"incremental": true,
	"esModuleInterop": true,
	"module": "esnext",
	"resolveJsonModule": true,
	"moduleResolution": "Node",
	"isolatedModules": true,
	"jsx": "preserve",
	"baseUrl": ".",
	"paths": {
	  "~/components": [
		"./components"
	  ],
	  "~/common": [
		"./common"
	  ]
	}
  },
  "include": [
	"next-env.d.ts",
	"**/*.ts",
	"**/*.tsx"
  ],
  "exclude": [
	"node_modules"
  ]
}

    },
    "./network": {
      "types": "./network.d.ts",
      "module": "./network.mjs",
      "main": "./network.mjs",
      "import": "./network.mjs",
      "react-native": "./network.js",
      "default": "./network.js"
    },
    "./storage": {
      "types": "./storage.d.ts",
      "module": "./storage.mjs",
      "main": "./storage.mjs",
      "import": "./storage.mjs",
      "react-native": "./storage.js",
      "default": "./storage.js"
    },
    "./transactions": {
      "types": "./transactions.d.ts",
      "module": "./transactions.mjs",

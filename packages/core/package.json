    "@swc-node/jest": "^1.5.2",
    "@types/bn.js": "5.1.0",
    "@types/common-tags": "1.8.1",
    "@types/jest": "28.1.6",
    "@types/jsdom": "20.0.0",
    "@types/node": "18.6.2",
    "@types/randombytes": "2.0.0",
    "@typescript-eslint/eslint-plugin": "5.31.0",

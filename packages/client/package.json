{
  "name": "@micro-stacks/client",
  "private": false,
  "version": "1.0.0-beta.11",
  "description": "Session client for Stacks apps using micro-stacks",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "import": "./dist/index.mjs",
  "browser": "./dist/index.global.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "main": "./dist/index.js",
      "module": "./dist/index.mjs",
      "import": "./dist/index.mjs",
      "browser": "./dist/index.global.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js",
      "main": "./dist/*.js",
      "module": "./dist/*.mjs",
      "import": "./dist/*.mjs",
      "browser": "./dist/*.global.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --config ../../configs/tsup.config.ts --format=cjs,esm,iife",
    "typecheck": "tsc --noEmit --target esnext",
    "dev:publish": "yalc publish --push",
    "dev:build": "pnpm build && yalc publish --push",
    "publish:beta": "pnpm publish --tag beta",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "lint:eslint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "lint:prettier": "prettier --check \"src/**/*.{ts,tsx}\" *.js",
    "lint:prettier:fix": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "engines": {
    "node": ">=14"
  },
  "prettier": "@stacks/prettier-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aulneau/micro-stacks-monorepo.git"
  },
  "keywords": [
    "stacks",
    "web3",
    "small-bundle"
  ],
  "author": "Thomas Osmonson",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fungible-systems/micro-stacks-monorepo/issues"
  },
  "homepage": "https://github.com/fungible-systems/micro-stacks-monorepo",
  "devDependencies": {
    "@stacks/stacks-blockchain-api-types": "4.1.0",
    "@types/common-tags": "1.8.1",
    "@types/jest": "28.1.6",
    "@types/jsdom": "16.2.14",
    "tsup": "6.1.3",
    "type-fest": "^2.16.0"
  },
  "dependencies": {
    "fast-deep-equal": "3.1.3",
    "micro-stacks": "workspace:*",
    "tiny-invariant": ">=1.2.0",
    "zustand": "4.0.0-rc.3"
  }
}
